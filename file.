	            if (match) {
	              args = args.slice(match[0].length).trim();
	              args += ', ' + match[0];
	            } else {
	              args += ', 0.5';
	            }
	          }
	          return prefix + _this.name + '(' + args + ')' + after;
	        };
	      })(this)).join(' ');
	    };

	    return CrossFade;

	  })(Value);

