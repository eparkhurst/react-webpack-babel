/* 169 */
/***/ function(module, exports, __webpack_require__) {

	'use strict';


	var yaml = __webpack_require__(170);


	module.exports = yaml;


/***/ },
/* 170 */
/***/ function(module, exports, __webpack_require__) {

	'use strict';


	var loader = __webpack_require__(171);
	var dumper = __webpack_require__(199);


	function deprecated(name) {
	  return function () {
	    throw new Error('Function ' + name + ' is deprecated and cannot be used.');
	  };
	}


	module.exports.Type                = __webpack_require__(177);
	module.exports.Schema              = __webpack_require__(176);
	module.exports.FAILSAFE_SCHEMA     = __webpack_require__(180);
	module.exports.JSON_SCHEMA         = __webpack_require__(179);
	module.exports.CORE_SCHEMA         = __webpack_require__(178);
	module.exports.DEFAULT_SAFE_SCHEMA = __webpack_require__(175);
	module.exports.DEFAULT_FULL_SCHEMA = __webpack_require__(194);
	module.exports.load                = loader.load;
	module.exports.loadAll             = loader.loadAll;
	module.exports.safeLoad            = loader.safeLoad;
	module.exports.safeLoadAll         = loader.safeLoadAll;
	module.exports.dump                = dumper.dump;
	module.exports.safeDump            = dumper.safeDump;
	module.exports.YAMLException       = __webpack_require__(173);

	// Deprecated schema names from JS-YAML 2.0.x
	module.exports.MINIMAL_SCHEMA = __webpack_require__(180);
	module.exports.SAFE_SCHEMA    = __webpack_require__(175);
	module.exports.DEFAULT_SCHEMA = __webpack_require__(194);

	// Deprecated functions from JS-YAML 1.x.x
